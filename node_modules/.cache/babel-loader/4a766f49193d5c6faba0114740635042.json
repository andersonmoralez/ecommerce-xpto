Product(event) {\n      event.preventDefault();\n      this.$emit(\"submit-form\", this.product);\n    }\n  },\n  created: function created() {\n    var _this = this;\n\n    this.loading = true;\n    axios.get(\"\".concat(process.env.VUE_APP_BASE_URL, \"/products\")).then(function (response) {\n      // getting all products and getting the uniq value for\n      // productCategory and returning the productCategory property only\n      _this.categories = _.uniqBy(_.map(response.data, function (object) {\n        return _.pick(object, [\"productCategory\"]);\n      }), \"productCategory\");\n\n      _this.categories.push({\n        productCategory: \"Create New\"\n      }); // getting all products and getting the uniq value for\n      // productSeller and returning the productSeller property only\n\n\n      _this.sellers = _.uniqBy(_.map(response.data, function (object) {\n        return _.pick(object, [\"productSeller\"]);\n      }), \"productSeller\");\n\n      _this.sellers.push({\n        productSeller: \"Create New\"\n      });\n\n      _this.loading = false;\n    }).catch(function (error) {\n      console.log(error);\n      errorToaster(\"Error while fetching products\", \"\");\n    });\n  }\n};",{"version":3,"sources":["ProductForm.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqEA,OAAA,KAAA,MAAA,OAAA;AACA,SAAA,YAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,aADA;AAEA,EAAA,KAAA,EAAA,CAAA,SAAA,CAFA;AAGA,EAAA,IAHA,kBAGA;AACA,WAAA;AACA,MAAA,MAAA,EAAA,EADA;AAEA,MAAA,oBAAA,EAAA,EAFA;AAGA,MAAA,kBAAA,EAAA,EAHA;AAIA,MAAA,UAAA,EAAA,EAJA;AAKA,MAAA,OAAA,EAAA,EALA;AAMA,MAAA,OAAA,EAAA;AANA,KAAA;AAQA,GAZA;AAaA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,yBACA,KADA,EACA;AACA,MAAA,KAAA,CAAA,cAAA;AACA,WAAA,KAAA,CAAA,aAAA,EAAA,KAAA,OAAA;AACA;AAJA,GAbA;AAmBA,EAAA,OAnBA,qBAmBA;AAAA;;AACA,SAAA,OAAA,GAAA,IAAA;AACA,IAAA,KAAA,CACA,GADA,WACA,OAAA,CAAA,GAAA,CAAA,gBADA,gBAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA;AACA;AACA,MAAA,KAAA,CAAA,UAAA,GAAA,CAAA,CAAA,MAAA,CACA,CAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,EAAA,UAAA,MAAA,EAAA;AACA,eAAA,CAAA,CAAA,IAAA,CAAA,MAAA,EAAA,CAAA,iBAAA,CAAA,CAAA;AACA,OAFA,CADA,EAIA,iBAJA,CAAA;;AAMA,MAAA,KAAA,CAAA,UAAA,CAAA,IAAA,CAAA;AAAA,QAAA,eAAA,EAAA;AAAA,OAAA,EATA,CAWA;AACA;;;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,CAAA,CAAA,MAAA,CACA,CAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,EAAA,UAAA,MAAA,EAAA;AACA,eAAA,CAAA,CAAA,IAAA,CAAA,MAAA,EAAA,CAAA,eAAA,CAAA,CAAA;AACA,OAFA,CADA,EAIA,eAJA,CAAA;;AAOA,MAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,aAAA,EAAA;AAAA,OAAA;;AAEA,MAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,KAzBA,EA0BA,KA1BA,CA0BA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,MAAA,YAAA,CAAA,+BAAA,EAAA,EAAA,CAAA;AACA,KA7BA;AA8BA;AAnDA,CAAA","sourcesContent":["<template>\n    <div class=\"productForm\">\n        <form id=\"productCU\" @submit=\"actionProduct\" v-if=\"!loading\">\n                <p v-if=\"errors.length\">\n                  <b>Please correct the following error(s):</b>\n                  <ul>\n                    <li v-for=\"error in errors\" :key=\"error\">{{ error }}</li>\n                  </ul>\n                </p>\n\n              <div class=\"form-group\">\n                <label for=\"productName\">Product Name</label>\n                <input type=\"text\" class=\"form-control\" id=\"productName\" v-model=\"product.productName\" name=\"productName\" aria-describedby=\"emailHelp\" placeholder=\"Enter Product Name\">\n                <small id=\"emailHelp\" class=\"form-text text-muted\">We'll never share your email with anyone else.</small>\n              </div>\n              <div class=\"form-group\">\n                <label for=\"productCategory\">Product Category</label>\n                <select class=\"form-control\"  v-if=\"product.productCategory !== 'Create New'\" id=\"productCategory\" name=\"productCategory\" v-model=\"product.productCategory\">\n                    <option \n                      v-for=\"(category, index) in categories\" :key=\"index\" \n                      v-bind:value=\"category.productCategory\" \n                      >{{category.productCategory}}\n                    </option>\n                </select>\n                <input type=\"password\" class=\"form-control\" id=\"productCategroyDummy\"  v-if=\"product.productCategory === 'Create New'\" placeholder=\"Password\" name=\"productCategroyDummy\" v-model=\"product.productCategroyDummy\">\n              </div>\n              <div class=\"form-group\">\n                <label for=\"productSeller\">Product Seller</label>\n                <select class=\"form-control\"  v-if=\"product.productSeller !== 'Create New'\" id=\"productSeller\" name=\"productSeller\" v-model=\"product.productSeller\">\n                    <option \n                      v-for=\"(seller, index) in sellers\" :key=\"index\" \n                      v-bind:value=\"seller.productSeller\" \n                      >{{seller.productSeller}}\n                    </option>\n                </select>\n                <input type=\"password\" class=\"form-control\" id=\"productSellerDummy\"  v-if=\"product.productSeller === 'Create New'\" placeholder=\"Password\" name=\"productSellerDummy\" v-model=\"product.productSellerDummy\">\n              </div>\n              <div class=\"row\">\n                <div class=\"col\">\n                  <div class=\"form-group\">\n                    <label for=\"isBestProduct\">Best Product</label>\n                    <input type=\"checkbox\" class=\"form-control\" id=\"isBestProduct\" name=\"isBestProduct\" v-model=\"product.isBestProduct\" placeholder=\"Password\">\n                  </div>\n                </div>\n                <div class=\"col\">\n                    <div class=\"form-group\">\n                      <label for=\"isTopProduct\">Top Product</label>\n                      <input type=\"checkbox\" class=\"form-control\" id=\"isTopProduct\" name=\"isTopProduct\" placeholder=\"Password\" v-model=\"product.isTopProduct\">\n                    </div>\n                </div>\n                <div class=\"col\">\n                  <div class=\"form-group\">\n                    <label for=\"productRating\">Product Rating</label>\n                    <input type=\"password\" class=\"form-control\" id=\"productRating\" placeholder=\"Password\">\n                  </div>\n                </div>\n              </div>\n              <div class=\"form-group\">\n                <label for=\"productImage\">Product Image</label>\n                <input type=\"text\" class=\"form-control\" id=\"productImage\" v-model=\"product.productImage\" placeholder=\"Product Image URL\">\n              </div>\n              <br>\n              <button type=\"submit\" class=\"btn buttonGreen\">Save changes</button>\n        </form>\n\n        <div class=\"lds-dual-ring\" v-if=\"loading\"></div>\n    </div>\n</template>\n<script>\nimport axios from \"axios\";\nimport {errorToaster} from \"../../shared/service/ErrorHandler.js\"\nexport default {\n  name: \"productForm\",\n  props: [\"product\"],\n  data() {\n    return {\n      errors: [],\n      productCategroyDummy: \"\",\n      productSellerDummy: \"\",\n      categories: [],\n      sellers: [],\n      loading: false\n    };\n  },\n  methods: {\n    actionProduct(event) {\n      event.preventDefault();\n      this.$emit(\"submit-form\", this.product);\n    }\n  },\n  created() {\n    this.loading = true;\n    axios\n      .get(`${process.env.VUE_APP_BASE_URL}/products`)\n      .then(response => {\n        // getting all products and getting the uniq value for\n        // productCategory and returning the productCategory property only\n        this.categories = _.uniqBy(\n          _.map(response.data, function(object) {\n            return _.pick(object, [\"productCategory\"]);\n          }),\n          \"productCategory\"\n        );\n        this.categories.push({ productCategory: \"Create New\" });\n\n        // getting all products and getting the uniq value for\n        // productSeller and returning the productSeller property only\n        this.sellers = _.uniqBy(\n          _.map(response.data, function(object) {\n            return _.pick(object, [\"productSeller\"]);\n          }),\n          \"productSeller\"\n        );\n\n        this.sellers.push({ productSeller: \"Create New\" });\n\n        this.loading = false;\n      })\n      .catch(error => {\n        console.log(error);\n        errorToaster(\"Error while fetching products\", \"\");\n      });\n  }\n};\n</script>\n<style lang=\"scss\">\n.productForm {\n  div {\n    text-align: start;\n  }\n  #productCU {\n    div {\n      text-align: start;\n    }\n    button {\n      text-align: center;\n    }\n  }\n}\n\n.lds-dual-ring {\n  display: inline-block;\n  width: 64px;\n  height: 64px;\n}\n.lds-dual-ring:after {\n  content: \" \";\n  display: block;\n  width: 46px;\n  height: 46px;\n  margin: 1px;\n  border-radius: 50%;\n  border: 5px solid #fff;\n  border-color: #41b883 transparent #41b883 transparent;\n  animation: lds-dual-ring 1.2s linear infinite;\n}\n@keyframes lds-dual-ring {\n  0% {\n    transform: rotate(0deg);\n  }\n  100% {\n    transform: rotate(360deg);\n  }\n}\n</style>"],"sourceRoot":"src/components/products/actions"}]}